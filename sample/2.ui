
include dark.ui

//

Clone Panel
{
    Name pnl_login
    RectWH 0 0 300 200
    Anchor xy 0.5 0.4
    Padding 10
    Rounding 10

    TextAnchor xy 0.5 0
    Text Login

    Clone Text
    {
        Name txt_user
        RectWH 10 60 100 30
        TextAlignW right
        Text User
    }
    Clone txt_user
    {
        Name txt_pw
        Y 100
        Text Password
    }
    Clone Edit
    {
        Name ed_user
        RectWH 130 60 150 30
        Rounding 5
    }
    Clone ed_user
    {
        Name ed_pw
        Y 100
        Password *
    }

    Clone Button
    {
        Name btn_ok
        RectWH 10 10 100 30
        Anchor xy 0.5 1
        Text OK 
    }
}

